<!-- Line item - Voice -->

<div class="line-item large contract {{class}}">
    <div class="main">
        <div class="waveform" id="{{siri_id}}"></div>
        <div class="contact-information">
            <div class="communication-title">
                {% if is_outgoingActive %}
                    calling<em>.</em><em>.</em><em>.</em>
                {% elseif is_outgoingFailed %}
                    no answer
                {% else %}

                {% endif %}
            </div>
            <div class="name">
                {{contact_name}}
            </div>
            <div class="company">
                {{company_name}}
            </div>
        </div>
        <div class="communication-icon">
            <svg version="1.1" id="reply-icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 85.6 88.4" style="enable-background:new 0 0 85.6 88.4;" xml:space="preserve">
                <style type="text/css">
                    .st0 {
                        stroke-width: 0.9904;
                        stroke-linecap: round;
                        stroke-linejoin: round;
                        stroke-miterlimit: 10;
                    }
                    .st1 {
                        stroke-miterlimit: 10;
                    }
                </style>
                <polygon class="st1" points="83.5,20.3 76.4,20.3 80.2,1.5 61.6,22.8 70.7,22.8 65,41.7 "/>
                <path class="st0" d="M9.3,9.1c-7.1,7.2-7.5,16.3-7.5,16.3c1.8,8.8,11.7,25,23.7,37c12,12,28.2,21.9,37,23.7c0,0,9.1-0.4,16.3-7.5
                l0,0l4.2-4.2c0.6-0.6,0.6-1.5,0-2.1l-18-18c-0.6-0.6-1.5-0.6-2,0l-4.2,4.2l0,0l-4,4.9c0,0-7.6-1.6-18.2-12.3
                C26,40.7,24.3,33.2,24.3,33.2l5-4l0,0l0.1-0.1h0l0,0l4.1-4.1c0.6-0.6,0.6-1.5,0-2l-18-18c-0.6-0.6-1.5-0.6-2,0L9.3,9.1L9.3,9.1z"/>
            </svg>

            <svg version="1.1" id="end-call-icon" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 84.4 84.1" style="enable-background:new 0 0 84.4 84.1;" xml:space="preserve">
                <style type="text/css">
                    .st0 {
                        stroke-miterlimit: 10;
                    }
                </style>
                <path class="st0" d="M8.8,5.7C1.7,12.9,1.3,22,1.3,22C3.1,30.9,13,47,25,59c12,12,28.2,21.9,37,23.7c0,0,9.1-0.4,16.3-7.5l0,0
                l4.2-4.2c0.6-0.6,0.6-1.5,0-2.1l-18-18c-0.6-0.6-1.5-0.6-2,0l-4.2,4.2l0,0l-4,4.9c0,0-7.6-1.6-18.2-12.3
                C25.5,37.3,23.9,29.7,23.9,29.7l5-4l0,0l0.1-0.1h0l0,0l4.1-4.1c0.6-0.6,0.6-1.5,0-2l-18-18c-0.6-0.6-1.5-0.6-2,0L8.8,5.7L8.8,5.7z"
                />
            </svg>
        </div>
        {% if is_outgoingFailed %}
            <div class="message-reply-trigger show">
                <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 24.3 25.1" style="enable-background:new 0 0 24.3 25.1;" xml:space="preserve">
                    <style type="text/css">
                        .st0 {
                            stroke-miterlimit: 10;
                        }
                    </style>
                    <path class="st0" d="M12.5,0.8c-6,0-10.8,5-10.8,11.1c0,2.4,0.7,4.5,2,6.3l-2.4,5.6l5.5-2.5c1.7,1.1,3.6,1.7,5.7,1.7
                    c6,0,10.8-5,10.8-11.1S18.5,0.8,12.5,0.8z"/>
                </svg>
            </div>
            <div class="message-reply">
                <input id="now-playing-response" type="text" name="search" value="" placeholder="SEND A MESSAGE INSTEAD">
                <div class="send-button">
                    Send
                </div>
                <div class="x-dismiss">
                    <svg version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 146.2 149.2" style="enable-background:new 0 0 146.2 149.2;" xml:space="preserve">
                        <line class="st0" x1="19.6" y1="21.1" x2="128.7" y2="130.2"/>
                        <line class="st0" x1="128.7" y1="21.1" x2="19.6" y2="130.2"/>
                    </svg>
                </div>
            </div>
            <div class="sent-message">
               Message sent
            </div>
        {% endif %}
    </div>

