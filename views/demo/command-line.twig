  {% include 'demo/awesome-menu.twig' %}
  <div class="caret">
    >
  </div>
  <div class="panel1">
    {% if is_noChat %}
      <img src="images/panel-no-chat.png" alt="Panel1 FPO">
    {% else %}
      <img src="images/panel.png" alt="Panel1 FPO">
    {% endif %}
  </div>
  <div class="panel2">
    <img src="images/panel2.png" alt="Panel2 FPO">
  </div>
  <div class="panel3">
    {% if is_phase1 %}
      <img src="images/panel3-phase1.png" alt="Panel3 - Phase1 FPO">
    {% else %}
      <img src="images/panel3.png" alt="Panel3 FPO">
    {% endif %}
  </div>
  <div class="search-controls">
    <img src="images/search-controls.png" alt="Search controls FPO">
  </div>

  {% include 'demo/search-prompt.twig' with
    {
      'class' : 'call',
      'name' : 'Call'
    }
  %}

  {% include 'demo/search-prompt.twig' with
    {
      'class' : 'add-contact',
      'name' : 'Add-contact'
    }
  %}

  <div class="tooltips">
    <div class="tooltip call">
      Enter a person, group, or phone number
    </div>
    <div class="tooltip add-contact">
      {% if is_phase1 %}
        <span class="message1 show">Enter a contact name</span>
      {% else %}
        <span class="message1 show">Enter a name or number</span>
      {% endif %}
      <span class="message2">We couldn’t find any results for “james blue”</span>
      <div class="cta-panel">
        <div class="button">
          Create Contact
        </div>
      </div>
    </div>
  </div>

